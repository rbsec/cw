#!/cw
path /bin:/usr/bin:/sbin:/usr/sbin:<env>
random cyan:green:purple:grey+
base random
digit random+:default
match random+:default /
match random&:default :
match random&:default .
match random&:default -
match random&:default #
ifnarg --help:--version
token random&:default 0 32
ifnarg -a:-all
nopipe
