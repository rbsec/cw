#!/cw
path /bin:/usr/bin:/sbin:/usr/sbin:/usr/local/bin:/usr/local/sbin:<env>
random cyan:green:purple
base random
digit random+:default
match random+:default '
match white:default :
match white:default .
match white:default -
match white:default /
match white:default |
match white:default [
match white:default ]
match white:default (
match white:default )
