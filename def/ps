#!/cw
path /bin:/usr/bin:/sbin:/usr/sbin:<env>
usepty
nobackground
random cyan:green:purple
base random
digit random+:default
match random+:default .
match random+:default -
match random+:default *
match random&:default :
match white:default /
match white:random+ [
match white:default ]
match white:random+ (
match white:default )
match white:random+ {
match white:default }
match white:default ?
match white:default tty
match white:default root
