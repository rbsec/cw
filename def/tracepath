#!/cw
path /sbin:/usr/sbin:/bin:/usr/bin:<env>
random purple:cyan:green:grey+
base random
digit random+:default
match random+:default .
match random+:default :
match random+:default *
match random&:default (
match random&:default )
match random+:random& [
match random+:default ]
match random&:default =
