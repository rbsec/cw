#!/cw
path /sbin:/usr/sbin:/bin:/usr/bin:<env>
base grey+
ifnarg <none>:-i:-ki
digit cyan:default
match white:none USER
ifnarg -i:-ki
match white:default -
match white:default /
match grey:default .
match cyan+:default :
match cyan+:default |
match cyan:default [
match cyan:default ]
match white:default (
match white:default )
