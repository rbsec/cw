#!/cw
path /sbin:/usr/sbin:/bin:/usr/bin:<env>
base cyan
digit cyan+:default
match blue+:default .
match blue+:default [
match blue+:default ]
match blue+:default <
match blue+:default >
match green+:default {
match green+:default }
match grey:default (
match grey:default )
match green+:default |
match green+:default :
match green+:default *
match green+:default +
match yellow:none Kernel
match white:none Destination
match white:default default
