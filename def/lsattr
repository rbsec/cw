#!/cw
path /bin:/usr/bin:/sbin:/usr/sbin:<env>
base cyan
match white:default :
match white:default ;
match green+:default .
match cyan+:default -
match cyan+:default /
match green+:default [
match green+:default ]
match green+:default (
match green+:default )
ifnarg --help
token green+:default 0 32
