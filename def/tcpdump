#!/cw
path /bin:/usr/bin:/sbin:/usr/sbin:/usr/local/bin:/usr/local/sbin:<env>
usepty
base cyan
digit cyan+:default
match grey:default .
match white:default :
match yellow:default ,
match yellow:default <
match yellow:default >
match blue+:default [
match blue+:default ]
match blue+:default (
match blue+:default )
match white:default tcpdump
match green+:default tcp
match green+:default udp
