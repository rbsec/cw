#!/cw
path /bin:/usr/bin:/sbin:/usr/sbin:<env>
ifnarg -c:-m:-l:-L:-w:--bytes:--chars:--lines:--max-line-length:--words
random purple:cyan:grey
base random
digit random&:default
match random+:random /
match random+:default .
match random+:default -
match random&:default <
match random&:default >
match random&:random+ [
match random&:default ]
ifarg-else
nopipe
