#!/cw
path /bin:/usr/bin:/sbin:/usr/sbin:<env>
base cyan
digit cyan+:default
match blue+:default .
match blue+:default [
match blue+:default ]
match blue+:default <
match blue+:default >
match green+:default {
match green+:default }
match grey:default (
match grey:default )
match cyan+:default -
match yellow:default /
match green+:default :
match green+:default *
match yellow:none Active
match green+:default ESTABLISHED
match green+:default TIME_WAIT
match green+:default DGRAM
match yellow:none Kernel
match white:none Destination
match white:default default
