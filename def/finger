#!/cw
path /bin:/usr/bin:/sbin:/usr/sbin:/usr/local/bin:/usr/local/sbin:<env>
base white
digit grey+:default
match grey+:default .
match grey:default :
match grey:default /
match grey:default -
match cyan+:default *
match grey:default [
match grey:default ]
match cyan+:default (
match cyan+:default )
ifarg <none>
match cyan+:none Login
