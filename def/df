#!/cw
path /bin:/usr/bin:/sbin:/usr/sbin:<env>
random brown:green:cyan
ifarg --help
base grey+
match white:default -
ifarg-else
base random+
match default:grey+ %
match default:grey+ K 
match default:grey+ M 
match default:grey+ G 
match default:grey+ T 
match default:none K-blocks
match default:none k-blocks
match default:none -blocks
match random+:default Mounted on
ifarg <any>
digit white:grey+
match grey:grey+ /
match grey:grey+ .
match grey:grey+ :
match grey:grey+ <
match grey:grey+ >
match grey:grey+ (
match grey:grey+ )
match grey:grey+ [
match grey:grey+ ]
match white:grey+ `
match white:default '
