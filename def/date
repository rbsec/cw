#!/cw
path /bin:/usr/bin:/sbin:/usr/sbin:<env>
base green+
digit cyan+:default
match blue+:default :
match blue+:default /
match cyan:default .
match cyan+:default -
match cyan+:default _
match cyan+:default =
match green+:white [
match green+:default ]
match yellow:default %{?} 
ifnarg <none>
nopipe
